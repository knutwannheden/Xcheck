<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<!-- Note: Don't forget to provide an ID attribute to templates, without they will not be loaded! -->

  <templates>
    <template
      id="com.avaloq.tools.dslsdk.checkcfg.CheckCfg.template.CheckConfiguration"
      autoinsert="true"
      context="com.avaloq.tools.dslsdk.checkcfg.CheckCfg.kw_check"
      deleted="false"
      description="Creates a new empty check configuration"
      enabled="true"
      name="CheckConfiguration">check configuration "${label}" {
  ${cursor}
}</template>
    <template
      id="com.avaloq.tools.dslsdk.checkcfg.CheckCfg.template.ConfiguredCatalog"
      autoinsert="true"
      context="com.avaloq.tools.dslsdk.checkcfg.CheckCfg.kw_catalog"
      deleted="false"
      description="Creates a catalog configuration structure"
      enabled="true"
      name="ConfiguredCatalog">catalog ${catalogName:CrossReference(ConfiguredCatalog.catalog)} {
  ${cursor}
}</template>

  </templates>